{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://192.168.1.40:9200",
    //"Username": "admin",
    //"Password": "admin",
    "IndexName": "product-api"
  },

  "DatabaseSettings": {
    "ConnectionString": "mongodb://root:admin@192.168.1.40:27017/admin",
    "DatabaseName": "ProductDb"
  },
  "MassTransit": {
    "UseAutomaticCorrelation": true,
    "BusStartStartTimeoutSeconds": 30,
    "BusStartStopTimeoutSeconds": 30,
    "RetryLimitCount": 2,
    "InitialIntervalSeconds": 3,
    "IntervalIncrementSeconds": 3,
    "ConcurrencyLimit": 4,
    "BrokerName": "RabbitMq",
    "MassTransitBusOptions": [
      {
        "BrokerType": 1,
        "BusBrokerName": "RabbitMq",
        "HostName": "192.168.1.40", //pod ismi 
        "VirtualHost": "/",
        "UserName": "admin",
        "Password": "123456"
      }
    ]
  },
  "DistributedLockOption": {
    "DistributedLockType": 1,
    "BaseUrl": "192.168.1.40",
    "Port": 6379,
    "ExpiryTimeFromSeconds": 3,
    "WaitTimeFromSeconds": 5,
    "RetryTimeFromMilliseconds": 300,
    "LogLockingProcess": true
  }
}
